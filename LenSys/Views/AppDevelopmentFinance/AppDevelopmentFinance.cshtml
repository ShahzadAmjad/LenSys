@model AppDevelopmentFinance

@{
    ViewBag.Title = "App Development Finance";
}
<div class="" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>Development Finance Application</h3>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 ">
                <div class="x_panel">

                    <div class="x_title">
                        <h2>Additional Loan Details</h2>
                        <div class="clearfix"></div>
                    </div>

                    @*Add Indvidual/Busniess/Lead Button*@
                    <div>
                        <a asp-controller="AppDevelopmentFinance" asp-action="AddIndividual" id="AddBusniessBtn" class="btn btn-primary mr-1">Add Individual</a>
                        <a asp-controller="AppDevelopmentFinance" asp-action="AddBusniess" id="AddBusniessBtn" class="btn btn-success mr-1">Add Business </a>
                        @*<a asp-controller="AppAssetFinance" asp-action="AddLead"  id="leadBtn" class="btn bg-danger mr-1">Add Lead</a>*@
                    </div>
                    <br />

                    @*All Individuals Table*@
                    <div class="x_title">
                        <h2>Individuals</h2>

                        <div class="clearfix"></div>
                    </div>

                    <div class="mtop20">
                        <table id="tblIndividuals" class="table table-bordered table-hover table-striped">
                            <thead class="thead-light">
                                <tr>
                                    <th id="id">Id</th>
                                    <th id="IndividualName" class="hideCol hideHideCol_2">Name</th>

                                    <th>Edit</th>
                                    <th>Delete</th>
                                </tr>
                            </thead>
                            <tbody>
                                @*@if ((List<AppBusniessFinanceSecurityDetails>)ViewData["SecurityDetailsList"] != null)*@
                                @if (Model.individuals != null)
                                {
                                    @if (Model.individuals.Count != 0)
                                    {
                                        @foreach (var individual in Model.individuals)

                                        {
                                            var tm = "#myModal" + individual.IndividualId;
                                            var mid = "myModal" + individual.IndividualId;
                                            <tr>
                                                <td>@individual.IndividualId</td>
                                                @*<td>Temporary Field</td>*@
                                                <td>@individual.personalDetails.FirstName</td>

                                                <td>
                                                    <a asp-controller="AppDevelopmentFinance" asp-action="EditIndividual" asp-route-Id="@individual.IndividualId" id="AddIndividualPersonalDetails" class="btn btn-primary btn-sm">
                                                        Default
                                                    </a>

                                                </td>

                                                <td>
                                                    <button type="button" style="width:80px;" class="btn btn-danger btn-sm" data-toggle="modal" data-target="@tm">
                                                        Delete
                                                    </button>

                                                    @*Delete Model for Individual*@
                                                    <div class="modal fade" id="@mid" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                                        <div class="modal-dialog">
                                                            <div class="modal-content">
                                                                <div class="modal-header">
                                                                    @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>*@
                                                                    <h4 class="modal-title text-center" id="myModalLabel">Delete Individual</h4>
                                                                    <button type="button" class="close " data-dismiss="modal" aria-hidden="true">&times;</button>
                                                                </div>
                                                                <div class="modal-body">
                                                                    Are you sure want to delete this Individual?
                                                                </div>
                                                                <div class="modal-footer">

                                                                    <a asp-controller="AppDevelopmentFinance" asp-action="DeleteIndividual" asp-route-Id="@individual.IndividualId" id="deleteBtn" class="btn bg-danger mr-1">Delete</a>
                                                                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                        }
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                    <br />

                    @*All Busniess Table*@

                    <div class="x_title">
                        <h2>Businesses</h2>

                        <div class="clearfix"></div>
                    </div>

                    <div class="mtop20">
                        <table id="tblBusniesses" class="table table-bordered table-hover table-striped">
                            <thead class="thead-light">
                                <tr>
                                    <th id="id">Id</th>
                                    <th id="CompanyName" class="hideCol hideHideCol_2">Company</th>
                                    <th>Edit</th>
                                    <th>Delete</th>
                                </tr>
                            </thead>
                            <tbody>
                                @if (Model.busniesses != null)
                                {
                                    @*@if ((List<AppBusniessFinanceSecurityDetails>)ViewData["SecurityDetailsList"] != null)*@
                                    @if (Model.busniesses.Count != 0)
                                    {
                                        @foreach (var busniess in Model.busniesses)

                                        {
                                            var tm = "#myModal" + busniess.BusniessId;
                                            var mid = "myModal" + busniess.BusniessId;
                                            <tr>
                                                <td>@busniess.BusniessId</td>
                                                @*<td>Temporary field</td>*@
                                                <td>@busniess.busniessDetails.CompanyBusniessName</td>

                                                <td>
                                                    <a asp-controller="AppDevelopmentFinance" asp-action="EditBusniess" asp-route-Id="@busniess.BusniessId" id="AddIndividualPersonalDetails" class="btn btn-primary btn-sm">
                                                        Default
                                                    </a>

                                                </td>

                                                <td>
                                                    <button type="button" style="width:80px;" class="btn btn-danger btn-sm" data-toggle="modal" data-target="@tm">
                                                        Delete
                                                    </button>

                                                    @*Delete Model for Individual*@
                                                    <div class="modal fade" id="@mid" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                                        <div class="modal-dialog">
                                                            <div class="modal-content">
                                                                <div class="modal-header">
                                                                    @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>*@
                                                                    <h4 class="modal-title text-center" id="myModalLabel">Delete Individual</h4>
                                                                    <button type="button" class="close " data-dismiss="modal" aria-hidden="true">&times;</button>
                                                                </div>
                                                                <div class="modal-body">
                                                                    Are you sure want to delete this Busniess?
                                                                </div>
                                                                <div class="modal-footer">

                                                                    <a asp-controller="AppDevelopmentFinance" asp-action="DeleteBusniess" asp-route-Id="@busniess.BusniessId" id="deleteBtn" class="btn bg-danger mr-1">Delete</a>
                                                                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                        }
                                    }
                                }
                            </tbody>
                        </table>
                    </div>

                    <br />
                    @*Development Details*@
                    @*<div class="x_title">
                        <h2>Development Details</h2>
                        <div class="clearfix"></div>
                    </div>*@
                    <div class="x_content">
                        <br />
                        <form id="AppDevelopmentFinanceForm" data-parsley-validate class="form-horizontal form-label-left" asp-controller="AppDevelopmentFinance" asp-action="AppDevelopmentFinance" method="post">

                            @*Security Details table*@
                            <div class="x_title">
                                <h2>Security Details</h2>
                                <div class="clearfix"></div>
                            </div>
                            @*Security Detail Button*@
                            <div class="mtop20">
                                <button type="button" class="btn btn-success" data-toggle="ajax-modal" id="AddSecuritybtn" name="AddSecuritybtn"
                                        data-target="#AddSecurityDetailModalDevelopment" data-url="@Url.Action("AddSecurityDetail")">
                                    Add New Security Details
                                </button>
                            </div>
                            @*Security Detail Modal*@
                            <div class="mtop20">
                                <table id="tblSecurityDetails" class="table table-bordered table-hover table-striped">
                                    <thead class="thead-light">
                                        <tr>
                                            <th id="id">Id</th>
                                            <th id="propertyType" class="hideCol hideHideCol_2">Description Of Property</th>
                                            <th id="pameOfPropertyOwner" class="hideCol hideHideCol_3">Name Of Property Owner</th>
                                            <th id="propertyvalue" class="hideCol hideHideCol_3">City</th>
                                            <th>Edit</th>
                                            <th>Delete</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model.securityDetails != null)
                                        {
                                            @if (Model.securityDetails.Count != 0)
                                            {
                                                @foreach (var securityDetails in Model.securityDetails)

                                                {
                                                    var tm = "#myModal" + securityDetails.SecurityDetailsId;
                                                    var mid = "myModal" + securityDetails.SecurityDetailsId;
                                                    <tr>
                                                        <td>@securityDetails.SecurityDetailsId</td>
                                                        <td>@securityDetails.DescriptionOfProperty</td>
                                                        <td>@securityDetails.NameOfPropertyOwner</td>
                                                        <td>@securityDetails.City</td>

                                                        <td>
                                                            <button type="button" style="width:80px;" class="btn btn-primary btn-sm" data-toggle="Edit-modal" id="EditSecuritybtn" name="EditSecuritybtn"
                                                                    data-target="#EditSecurityDetailModalDevelopment" onclick="EditSecurityOnclick(@securityDetails.SecurityDetailsId)" data-url="@Url.Action("EditSecurityDetail")">
                                                                Edit
                                                            </button>
                                                        </td>

                                                        <td>

                                                            <button type="button" style="width:80px;" class="btn btn-danger btn-sm" data-toggle="modal" data-target="@tm">
                                                                Delete
                                                            </button>


                                                            <div class="modal fade" id="@mid" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                                                <div class="modal-dialog">
                                                                    <div class="modal-content">
                                                                        <div class="modal-header">
                                                                            <h4 class="modal-title text-center" id="myModalLabel">Delete Security Detail</h4>
                                                                            <button type="button" class="close " data-dismiss="modal" aria-hidden="true">&times;</button>
                                                                        </div>
                                                                        <div class="modal-body">
                                                                            Are you sure want to delete this Security?
                                                                        </div>
                                                                        <div class="modal-footer">

                                                                            <a asp-controller="AppDevelopmentFinance" asp-action="DeleteSecurityDetail" asp-route-Id="@securityDetails.SecurityDetailsId" id="deleteBtn" class="btn bg-danger mr-1">Delete</a>
                                                                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                        }
                                        </tbody>
                                </table>
                            </div>

                            @*Development Details*@
                        <div>
                            <div class="x_title">
                                <h2>Development Details</h2>
                                <div class="clearfix"></div>
                            </div>
                            <div class="item form-group" hidden >
                                <label class="col-form-label col-md-3 col-sm-3 label-align" asp-for="LoanId">
                                    Id
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input type="text" asp-for="LoanId" id="LoanId" name="LoanId" readonly class="form-control">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label class="col-form-label col-md-3 col-sm-3 label-align" asp-for="PurchasePrice">
                                    Purchase Price
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input type="text" asp-for="PurchasePrice" id="purchasePrice" name="purchasePrice" required="required" class="form-control">
                                </div>
                            </div>

                            <div class="item form-group">
                                <label asp-for="MarketValueOfLand" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Market Value Of Land <span class="required">*</span>
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input type="text" asp-for="MarketValueOfLand" id="marketValueOfLand" name="marketValueOfLand" required="required" class="form-control">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="ConstructionCost" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Construction Cost
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="ConstructionCost" id="constructionCost" name="constructionCost" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="TotalDevelopmentCost" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Total Development Cost
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="TotalDevelopmentCost" id="totalDevelopmentCost" name="totalDevelopmentCost" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="GrossDevelopmentValue" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Gross Development Value
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="GrossDevelopmentValue" id="grossDevelopmentValue" name="grossDevelopmentValue" class="form-control" type="text">
                                </div>
                            </div>


                            <div class="item form-group">
                                <label asp-for="LoanAmount" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Loan Amount
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="LoanAmount" id="loanAmount" name="loanAmount" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="LoanTermMonths" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Loan Term (Months)
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="LoanTermMonths" id="loanTermMonths" name="loanTermMonths" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="AmountOfExistingLoanToBe" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Amount Of Existing Loan To Be
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="AmountOfExistingLoanToBe" id="amountOfExistingLoanToBe" name="amountOfExistingLoanToBe" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="TargetCompletionDate" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Target Completion Date
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="TargetCompletionDate" id="TargetCompletionDate" name="TargetCompletionDate" class="date-picker form-control" placeholder="dd-mm-yyyy" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
                                    <script>
                                        function timeFunctionLong(input) {
                                            setTimeout(function () {
                                                input.type = 'text';
                                            }, 60000);
                                        }</script>
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="SourceOfDeposit" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Source Of Deposit
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="SourceOfDeposit" id="sourceOfDeposit" name="sourceOfDeposit" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="ExistStrategy" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Exist Strategy
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="ExistStrategy" id="existStrategy" name="existStrategy" class="form-control" type="text">
                                </div>
                            </div>
                        </div>

                            @*Construction Fee*@

                            <div>
                            <div class="x_title">
                                <h2>Construction Fee</h2>
                                <div class="clearfix"></div>
                            </div>

                            <div class="item form-group">
                                <label asp-for="Demolition" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Demolition
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="Demolition" id="demolition" name="demolition" class="form-control" type="text">
