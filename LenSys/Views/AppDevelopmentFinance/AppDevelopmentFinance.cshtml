    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

    <script>

        var PlaceHolderElement = $('#PlaceHolderHere');

        //Show modal on Add Button click
        $('button[data-toggle="ajax-modal" ]').click(function (event) {

            var url = $(this).data('url');

            $.get(url).done(function (data) {
                PlaceHolderElement.html(data);
                $('#AddSecurityDetailModalDevelopment').modal('show');
                //PlaceHolderElement.find('.modal').modal('show');
            })
        })



        function EditSecurityOnclick(routeId) {
            //alert(String(routeId));

            var url = $('#EditSecuritybtn').data('url') + "/" + routeId;

            $.get(url).done(function (data) {
                PlaceHolderElement.html(data);
                $('#EditSecurityDetailModalDevelopment').modal('show');

            })
        }

        //Save Modal Data Add Security
        PlaceHolderElement.on('click', '[data-save="modal"]', function (event) {

            var form = $('#AddSecurityDetailform');

            var actionurl = form.attr('action');

            var sendData = form.serialize();

            $.post(actionurl, sendData).done(function (data) {
                $('#AddSecurityDetailModalDevelopment').modal('hide');

            })
            window.location.reload();


        })

        //Save Edit Modal Data Add New Security detail
        PlaceHolderElement.on('click', '[data-save="Editmodal"]', function (event) {
            
            var form = $('#EditSecurityDetailform');

            var actionurl = form.attr('action');

            var sendData = form.serialize();

            $.post(actionurl, sendData).done(function (data) {
                $('#EditSecurityDetailModalDevelopment').modal('hide');
            })
            window.location.reload();
        })


    </script>
