                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="Insert1" id="insert1" name="insert1" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="Insert2" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Insert
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="Insert2" id="insert2" name="insert2" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="Insert3" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Insert
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="Insert3" id="insert3" name="insert3" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="Insert4" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Insert
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="Insert4" id="insert4" name="insert4" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="StampDuty" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Stamp Duty
                                </label>
                                <div class="col-md-6 col-sm-6 ">
