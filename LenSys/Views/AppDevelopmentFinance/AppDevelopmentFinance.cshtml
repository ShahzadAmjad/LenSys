                            </div>
                            <div class="item form-group">
                                <label asp-for="MaterialsCost" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Materials Cost
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="MaterialsCost" id="materialsCost" name="materialsCost" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="GasInstallation" class="col-form-label col-md-3 col-sm-3 label-align">
