                            <div class="item form-group">
                                <label asp-for="LandScapingCost" class="col-form-label col-md-3 col-sm-3 label-align">
                                    LandScaping Cost
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="LandScapingCost" id="landScapingCost" name="landScapingCost" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="Labour" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Labour
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="Labour" id="labour" name="labour" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="CIL" class="col-form-label col-md-3 col-sm-3 label-align">
                                    CIL
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="CIL" id="cIL" name="cIL" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="Insert1" class="col-form-label col-md-3 col-sm-3 label-align">
