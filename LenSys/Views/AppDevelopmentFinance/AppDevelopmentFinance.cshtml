                            @*Selling Costs*@

                            <div>
                            <div class="x_title">
                                <h2>Selling Cost</h2>
                                <div class="clearfix"></div>
                            </div>

                            <div class="item form-group">
                                <label asp-for="EstateAgentFees" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Estate Agent Fees
                                </label>
                                <div class="col-md-6 col-sm-6 ">
