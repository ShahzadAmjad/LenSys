                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="DocumentId" id="documentId" name="documentId" class="form-control" type="text">
                                </div>
                            </div>*@

                            <div style="margin-left:100px;">

                                <div class="form-check ">
                                    <input class="form-check-input  " asp-for="DocumentNameList" type="checkbox" value="" id="defaultCheck1">
                                    <label class="form-check-label " for="defaultCheck1">
                                        Financial accounts. Most recent filed annual accounts including Profit & Loss account. (required by default for Business Finance and Asset Finance)
                                    </label>
                                </div>


                                <br />

                                <div class="form-check">
                                    <input class="form-check-input" asp-for="DocumentNameList" type="checkbox" value="" id="defaultCheck2">
                                    <label class="form-check-label" for="defaultCheck2">
                                        Business bank statements. Last 6 months' business bank statements in PDF format (required by default for Business Finance and Asset Finance)
                                    </label>
                                </div>


                                <br />

                                <div class="form-check">
                                    <input class="form-check-input" asp-for="DocumentNameList" type="checkbox" value="" id="defaultCheck3">
                                    <label class="form-check-label" for="defaultCheck3">
                                        Personal bank statements. Last 6 months' personal bank statements in PDF format.
                                    </label>
                                </div>


                                <br />

                                <div class="form-check">
                                    <input class="form-check-input" asp-for="DocumentNameList" type="checkbox" value="" id="defaultCheck4">
                                    <label class="form-check-label" for="defaultCheck4">
                                        Aged debtor list
                                    </label>
                                </div>


                                <br />

                                <div class="form-check">
                                    <input class="form-check-input" asp-for="DocumentNameList" type="checkbox" value="" id="defaultCheck5">
                                    <label class="form-check-label" for="defaultCheck5">
                                        Aged creditor list
                                    </label>
                                </div>


