@model AppPropertyFinance

@{
    ViewBag.Title = "App Property Finance";
}
<div class="" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>Property Finance Application</h3>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 ">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Loan Details</h2>

                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <br />
                        <form id="AppPropertyFinanceForm" data-parsley-validate class="form-horizontal form-label-left" asp-controller="AppPropertyFinance" asp-action="AppPropertyFinance" method="post">

                            <div class="item form-group">
                                <label class="col-form-label col-md-3 col-sm-3 label-align" asp-for="LoanPurpose">
                                    Loan Purpose
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <select asp-for="LoanPurpose" id="loanPurpose" name="loanPurpose" class="form-control">
                                        <option>
                                            Commercial mortgage
                                        </option>
                                        <option>
                                            Buy to let (residential/commercial)
                                        </option>
                                        <option>
                                            Property development
                                        </option>
                                        <option>
                                            Bridging loan
                                        </option>
                                        <option>
                                            Unsecured finance
                                        </option>
                                    </select>
                                </div>
                            </div>

                            <div class="item form-group">
                                <label asp-for="LoanAmount" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Loan Amount <span class="required">*</span>
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input type="text" asp-for="LoanAmount" id="loanAmount" name="loanAmount" required="required" class="form-control">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="RateType" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Rate Type
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <select asp-for="RateType" id="RateType" name="RateType" class="form-control">
                                        <option>
                                            Fixed rate
                                        </option>
                                        <option>
                                            Variable rate
                                        </option>
                                        <option>
                                            Any
                                        </option>
                                    </select>
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="RepaymentType" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Repayment Type
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <select asp-for="RepaymentType" id="repaymentType" name="repaymentType" class="form-control">
                                        <option>
                                            Capital & interest
                                        </option>
                                        <option>
                                            Interest only
                                        </option>
                                        <option>
                                            Part & part
                                        </option>
                                    </select>
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="RepaymentTermMonths" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Repayment Term(Months)
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="RepaymentTermMonths" id="repaymentTermMonths" name="repaymentTermMonths" class="form-control" type="text">
                                </div>
                            </div>


                            <div class="item form-group">
                                <label asp-for="TargetCompletionDate" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Target Completion Date
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="TargetCompletionDate" id="targetCompletionDate" name="targetCompletionDate" class="date-picker form-control" placeholder="dd-mm-yyyy" type="text" required="required" onfocus="this.type='date'" onmouseover="this.type='date'" onclick="this.type='date'" onblur="this.type='text'" onmouseout="timeFunctionLong(this)">
                                    <script>
                                        function timeFunctionLong(input) {
                                            setTimeout(function () {
                                                input.type = 'text';
                                            }, 60000);
                                        }</script>
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="BrokerFee" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Broker Fee
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="BrokerFee" id="brokerFee" name="brokerFee" class="form-control" type="text">
                                </div>
                            </div>
                            <div class="item form-group">
                                <label asp-for="SourceOfDeposit" class="col-form-label col-md-3 col-sm-3 label-align">
                                    Source Of Deposit
                                </label>
                                <div class="col-md-6 col-sm-6 ">
                                    <input asp-for="SourceOfDeposit" id="sourceOfDeposit" name="sourceOfDeposit" class="form-control" type="text">
                                </div>
                            </div>



                            @*Security Details table will come here*@


                            <div class="x_title">
                                <h2>Security Details</h2>

                                <div class="clearfix"></div>
                            </div>
                            @*Related To partial View*@
                            <div name="PlaceHolderHere" id="PlaceHolderHere">
                                @*<partial name="_AddSecutityDetailBusniessPartialView" />*@
                            </div>

                            <div class="mtop20">
                                @*<button type="button" class="btn btn-success" data-toggle="modal" data-target="#createSecurityDetailModal">Add New Security Details</button>*@
                                @*Razor Modal POP Button*@
                                <button type="button" class="btn btn-success" data-toggle="ajax-modal" id="AddSecuritybtn" name="AddSecuritybtn"
                                        data-target="#AddSecurityDetailModalProperty" data-url="@Url.Action("AddSecurityDetail")">
                                    Add New Security Details
                                </button>
                            </div>

                            <div class="mtop20">
                                <table id="tblSecurityDetails" class="table table-bordered table-hover table-striped">
                                    <thead class="thead-light">
                                        <tr>
                                            <th id="id">Id</th>
                                            <th id="propertyType" class="hideCol hideHideCol_2">Property Type</th>
                                            <th id="pameOfPropertyOwner" class="hideCol hideHideCol_3">Name Of Property Owner</th>
                                            <th id="propertyvalue" class="hideCol hideHideCol_3">City</th>
                                            <th>Edit</th>
                                            <th>Delete</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if ((List<AppPropertyFinanceSecurityDetails>)ViewData["SecurityDetailsList"] != null)
                                        {
                                            @foreach (var securityDetails in (IEnumerable<AppPropertyFinanceSecurityDetails>)ViewData["SecurityDetailsList"])

                                            {
                                                var tm = "#myModal" + securityDetails.SecurityDetailsId;
                                                var mid = "myModal" + securityDetails.SecurityDetailsId;
                                                <tr>
                                                    <td>@securityDetails.SecurityDetailsId</td>
                                                    <td>@securityDetails.PropertyType</td>
                                                    <td>@securityDetails.NameOfPropertyOwner</td>
                                                    <td>@securityDetails.City</td>

                                                    <td>
                                                    @*<a asp-controller="AppPropertyFinance" asp-action="EditSecuritydetail" 
                                                       class="btn btn-primary " asp-route-id="@securityDetails.SecurityDetailsId">Edit</a></td>*@
                                                         <button type="button" style="width:80px;" class="btn btn-primary btn-sm" data-toggle="Edit-modal" id="EditSecuritybtn" name="EditSecuritybtn"
                                                                data-target="#EditSecurityDetailModalDevelopment" onclick="EditSecurityOnclick(@securityDetails.SecurityDetailsId)" data-url="@Url.Action("EditSecurityDetail")">
                                                            Edit
                                                        </button>
                                                    <td>

                                                        <button type="button" style="width:80px;" class="btn btn-danger btn-sm" data-toggle="modal" data-target="@tm">
                                                            Delete
                                                        </button>


                                                        <div class="modal fade" id="@mid" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                                            <div class="modal-dialog">
                                                                <div class="modal-content">
                                                                    <div class="modal-header">
                                                                        @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>*@
                                                                        <h4 class="modal-title text-center" id="myModalLabel">Delete Security Detail</h4>
                                                                        <button type="button" class="close " data-dismiss="modal" aria-hidden="true">&times;</button>
                                                                    </div>
                                                                    <div class="modal-body">
                                                                        Are you sure want to delete this Security?
                                                                    </div>
                                                                    <div class="modal-footer">

                                                                        <a asp-controller="AppPropertyFinance" asp-action="DeleteSecurityDetail" asp-route-Id="@securityDetails.SecurityDetailsId" id="deleteBtn" class="btn bg-danger mr-1">Delete</a>
                                                                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>

                            </div>
